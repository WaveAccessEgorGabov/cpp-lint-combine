#include <winver.h>

#define PRODUCTNAME_STR    "@PRODUCTNAME_STR_IMPL@"
#define PRODUCTVERSION_STR "@PRODUCTVERSION_STR_IMPL@"
#define FILEVERSION_STR     @FILEVERSION_STR_IMPL@

VS_VERSION_INFO VERSIONINFO
    FILEVERSION FILEVERSION_STR
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "ProductName",    PRODUCTNAME_STR
            VALUE "ProductVersion", PRODUCTVERSION_STR
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 0x4b0
    END
END
