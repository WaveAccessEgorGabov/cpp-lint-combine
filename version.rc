#include <winver.h>
#include "version.rsrc"

VS_VERSION_INFO VERSIONINFO
    FILEVERSION FILEVERSION_STR
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "ProductName",    PRODUCTNAME_STR
            VALUE "ProductVersion", PRODUCTVERSION_STR
        END
